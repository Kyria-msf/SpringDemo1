<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">
   
   
   
   <bean id="mathCheatObjectValue" class="AllInjectionInOne.MathCheat"></bean>
   
   <bean id = "scienceCheatObjectValue" class="AllInjectionInOne.scienceCheat"></bean>
   
   <bean id = "javaCheatObjectValue" class="AllInjectionInOne.JavaCheat"></bean>
   
   <bean id = "stu" class= "AllInjectionInOne.Student">
     <property name="cheat" ref = "javaCheatObjectValue"></property>
   </bean>
   
   
</beans>

