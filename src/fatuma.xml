<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">
   


<bean id="mathCheatObjectValue" class="Fatuma.MathCheat"></bean>
   
   <bean id = "scienceCheatObjectValue" class="Fatuma.ScienceCheat"></bean>
   
   <bean id = "javaCheatObjectValue" class="Fatuma.JavaCheat"></bean>
   
   <bean id = "cheatingus" class = "Fatuma.Student">
     <property name="cheat" ref = "javaCheatObjectValue"></property>
   </bean>

</beans>